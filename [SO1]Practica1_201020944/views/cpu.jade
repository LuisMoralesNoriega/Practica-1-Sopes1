        script(src='stylesheets/light-wave/template/assets/js/bootstrap.js')
        // EASING SCROLL SCRIPTS PLUGIN
        script(src='stylesheets/light-wave/template/assets/js/vegas/jquery.vegas.min.js')
        // VEGAS SLIDESHOW SCRIPTS
        script(src='stylesheets/light-wave/template/assets/js/jquery.easing.min.js')
        // FANCYBOX PLUGIN
        script(src='stylesheets/light-wave/template/assets/js/source/jquery.fancybox.js')
        // ISOTOPE SCRIPTS
        script(src='stylesheets/light-wave/template/assets/js/jquery.isotope.js')
        // VIEWPORT ANIMATION SCRIPTS
        script(src='stylesheets/light-wave/template/assets/js/appear.min.js')
        script(src='stylesheets/light-wave/template/assets/js/animations.min.js')
